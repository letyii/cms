{
	"name": "letyii/cms",
	"description": "LetYii CMS",
	"keywords": ["yii2", "framework", "cms", "letyii"],
	"homepage": "http://letyii.com",
	"type": "project",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/letyii/cms/issues",
		"source": "https://github.com/letyii/cms"
	},
	"minimum-stability": "alpha",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "dev-master",
		"yiisoft/yii2-bootstrap": "dev-master",
		"yiisoft/yii2-swiftmailer": "dev-master",
		"yiisoft/yii2-jui": "dev-master",
        "letyii/yii2-gii": "dev-master",
        "letyii/yii2-tinymce": "dev-master",
        "letyii/yii2-chart": "dev-master",
        "creocoder/yii2-nested-set-behavior": "dev-master",
        "kartik-v/bootstrap-fileinput": "dev-master",
        "kartik-v/dependent-dropdown": "dev-master",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-social": "dev-master",
        "kartik-v/yii2-field-range": "dev-master",
        "marciocamello/yii2-x-editable": "dev-master"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*"
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"runtime",
			"assets"
		],
		"executable": [
			"yii"
		]
	}
}
