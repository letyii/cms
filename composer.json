{
	"name": "letyii/cms",
	"description": "LetYii CMS",
	"keywords": ["yii2", "framework", "cms", "letyii"],
	"homepage": "http://letyii.com",
	"type": "project",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/letyii/cms/issues",
		"source": "https://github.com/letyii/cms"
	},
	"minimum-stability": "alpha",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-swiftmailer": "*",
		"yiisoft/yii2-jui": "*",
        "creocoder/yii2-nested-set-behavior": "dev-master",
        "marciocamello/yii2-x-editable": "dev-master"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*"
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"runtime",
			"assets"
		],
		"executable": [
			"yii"
		]
	}
}
